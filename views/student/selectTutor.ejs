<body>
  <div class="sidebar close_1">
    <div class="logo-details">
      <i class='bx bxs-chalkboard'></i>
      <span class="logo_name">LMS</span>
    </div>
    <ul class="nav-links">
      <li>
        <a href="#">
          <i class='bx bxs-dashboard'></i>
          <span class="link_name">Dashboard</span>
        </a>
        <ul class="sub-menu blank">
          <li><a class="link_name" href="">Dashboard</a></li>
        </ul>
      </li>
      <li>
        <div class="iocn-link">
          <a href="/courses">
            <i class='bx bx-collection'></i>
            <span class="link_name">Courses</span>
          </a>
          <ul class="sub-menu blank">
            <li><a class="link_name" href="courses.html">Courses</a></li>
          </ul>
        </div>
      </li>

      <li>
        <a href="#">
          <i class='bx bxs-bell'></i>
          <span class="link_name">Notifications</span>
        </a>
        <ul class="sub-menu blank">
          <li><a class="link_name" href="#">Notifications</a></li>
        </ul>
      </li>

      <li>
        <a href="/viewCourse/<%= courseName.CRN %>/assessments">
          <i class='bx bx-task'></i>
          <span class="link_name">Assesments</span>
        </a>
        <ul class="sub-menu blank">
          <li><a class="link_name" href="assesment.html">Assesments</a></li>
        </ul>
      </li>

      <li>
        <a href="#">
          <i class='bx bx-notepad'></i>
          <span class="link_name">Materials</span>
        </a>
        <ul class="sub-menu blank">
          <li><a class="link_name" href="notes.html">Materials</a></li>
        </ul>
      </li>

      <li>
        <a href="#">
          <i class='bx bx-bar-chart'></i>
          <span class="link_name">Grades</span>
        </a>
        <ul class="sub-menu blank">
          <li><a class="link_name" href="grades.html">Grades</a></li>
        </ul>
      </li>

      <li>
        <a href="#">
          <i class='bx bxs-message-dots'></i>
          <span class="link_name">Chats</span>
        </a>
        <ul class="sub-menu blank">
          <li><a class="link_name" href="chats.html">Chats</a></li>
        </ul>
      </li>

      <li>
        <a href="#">
          <i class='bx bx-calendar'></i>
          <span class="link_name">Tutor Reservation</span>
        </a>
