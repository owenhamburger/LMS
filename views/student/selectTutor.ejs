          <li><a class="link_name" href="reservetutor.html">Tutor Reservation</a></li>
        </ul>
      </li>

      <li>
        <a href="/logout">
          <i class='bx bx-log-out'></i>
          <span class="link_name">Log Out</span>
        </a>
        <ul class="sub-menu blank">
          <li><a class="link_name" href="#">Log Out</a></li>
        </ul>
      </li>

    </ul>
  </div>
  <section class="home-section">
    <div class="home-content">
      <i class='bx bx-menu'></i>
    </div>
      <h1 style="text-align: center; padding-top: 10px;">Reserve Tutor</h1>
    <section class="reserveTutorMainContainer">
      <div class="reserveTutorTop">
        <h2 style="text-align: center; font-size: 20px; margin-top: 30px;">Upcoming Reservation</h2>
        <table class="table table-hover">
          <thead class="thead-dark">
            <tr>
              <th scope="col">Date</th>
              <th scope="col">Time</th>
              <th scope="col">Tutor</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td rowspan="1">April 5th, Friday</td>
              <td>8am - 9am</td>
              <td>Amine G</td>
            </tr>

            <tr>
              <td rowspan="1">April 8th, Monday</td>
              <td>12pm - 1pm</td>
              <td>Owen H</td>
            </tr>

            <tr>
              <td rowspan="1">April 10th, Wednesday</td>
              <td>2pm - 3pm</td>
              <td>Fared F</td>
            </tr>
          </tbody>
        </table>
      </div>
        <div class="reserveTutorContainer">
          <h2 style="text-align: center; font-size: 20px; margin-top: 30px;">Select Tutor</h2>
          <div class="courseListContainer">
            <div class="courseList">
                <div class="card bg-light mb-3" style="max-width: 18rem;">
                  <div class="card-header" style="background-color: #343a40;"></div>
                  <div class="card-body text-primary">
                    <h5 class="card-title">Tutor 1</h5>
                    <a href="/viewCourse/<%= courseName.CRN %>/tutorReservation/tutorInfo" class="btn btn-dark" style="color: white; cursor: pointer;">Select Tutor</a>
                  </div>
                </div>
            </div>
            <div class="courseList">
                <div class="card bg-light mb-3" style="max-width: 18rem;">
                  <div class="card-header" style="background-color: #343a40;"></div>
                  <div class="card-body text-primary">
                    <h5 class="card-title">Tutor 2</h5>
                    <a href="/viewCourse/<%= courseName.CRN %>/tutorReservation/tutorInfo" class="btn btn-dark" style="color: white; cursor: pointer;">Select Tutor</a>
                  </div>
                </div>
            </div>
            <div class="courseList">
                <div class="card bg-light mb-3" style="max-width: 18rem;">
                  <div class="card-header" style="background-color: #343a40;"></div>
                  <div class="card-body text-primary">
                    <h5 class="card-title">Tutor 3</h5>
                    <a href="/viewCourse/<%= courseName.CRN %>/tutorReservation/tutorInfo" class="btn btn-dark" style="color: white; cursor: pointer;">Select Tutor</a>
                  </div>
                </div>
            </div>
          </div>
        </div>
    </section>
    <!-- <div class="home-content"> -->
    <h1 style="text-align: center;padding-top: 10px;"></h1>
  </section>

  <script src="../Public/sidebar.js" defer></script>

